{
  "name": "sd-fishing-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./assets/styles.css --minify",
    "build:js": "esbuild src/main.tsx --bundle --format=esm --target=es2020 --outfile=assets/main.js --sourcemap --loader:.tsx=tsx --loader:.ts=ts",
    "dev": "npm run dev:css & npm run dev:js",
    "dev:css": "tailwindcss -i ./src/styles/globals.css -o ./assets/styles.css --watch",
    "dev:js": "esbuild src/main.tsx --bundle --format=esm --target=es2020 --outfile=assets/main.js --sourcemap --watch --loader:.tsx=tsx --loader:.ts=ts",
    "typecheck": "tsc --noEmit",
    "test:ui": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@tanstack/virtual-core": "^3.13.12",
    "@types/node": "^20.14.2",
    "@types/react": "^19.1.17",
    "@types/react-dom": "^19.1.11",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.23.0",
    "json-schema-to-typescript": "^15.0.4",
    "postcss": "^8.5.6",
    "shadcn": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1"
  }
}
