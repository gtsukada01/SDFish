{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend",
  "installCommand": "cd frontend && npm install",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
